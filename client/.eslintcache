nt":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"29","messages":"30","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},"C:\\Users\\mavki\\Documents\\Bootcamp_Homework\\Week22-Homework\\goolge-books-react\\client\\src\\App.js",[],"C:\\Users\\mavki\\Documents\\Bootcamp_Homework\\Week22-Homework\\goolge-books-react\\client\\src\\components\\Header.js",[],"C:\\Users\\mavki\\Documents\\Bootcamp_Homework\\Week22-Homework\\goolge-books-react\\client\\src\\containers\\SearchPage\\index.js",["31"],"import React, {useState, useEffect} from \"react\";\r\nimport API from '../../utils/API';\r\nimport './index.css';\r\n\r\n\r\nfunction SearchForm(props) {\r\n\r\n  const [books, setBooks] = useState([])\r\n  const [formObject, setFormObject] = useState({})\r\n\r\n  function handleInputChange(event) {\r\n    const {name, value} = event.target;\r\n    setFormObject({...formObject, [name]: value})\r\n  }\r\n\r\n  function handleFormSubmit(event) {\r\n    event.preventDefault();\r\n\r\n    let query = formObject.search;\r\n    if (!query) return\r\n\r\n    API.searchGoogle(query).then(response => {\r\n      console.log(response);\r\n      let results = response.items\r\n      results = results.map((results) => {\r\n        results = {\r\n          key: results.id,\r\n          id: results.id,\r\n          title: results.volumeInfo.title,\r\n          author: results.volumeInfo.authors,\r\n          publisher: results.volumeInfo.publisher,\r\n          description: results.volumeInfo.description,\r\n          image: results.volumeInfo.imageLinks.thumbnail,\r\n          link: results.volumeInfo.infoLink\r\n        }\r\n        return(results)\r\n      })\r\n      setBooks(results)\r\n    })\r\n  }\r\n\r\n  function handleSaveBtn(event) {\r\n    event.preventDefault();\r\n\r\n    let savedBooks = books.find(book => book.id === event.target.id)\r\n    console.log(savedBooks);\r\n    API.savebook(savedBooks);\r\n  }\r\n\r\n  return (\r\n      <div className={'container'}>\r\n        <div className={'row'}>\r\n          <div className={'card col-12 mt-3 p-3 text-center'}>\r\n            <h1> (React) Google Books Search </h1>\r\n            <h5> Search and Save Books of Interest</h5>\r\n          </div>\r\n        </div>\r\n        <div className={'row'}>\r\n          <div className={'card col-12 p-3 mt-5'}>\r\n            <form>\r\n              <div className=\"form-group\">\r\n                <label htmlFor=\"search\">Book:</label>\r\n                <input\r\n                    onChange={handleInputChange}\r\n                    value={props.value}\r\n                    name=\"search\"\r\n                    type=\"text\"\r\n                    className=\"form-control\"\r\n                    placeholder=\"Search For a Book\"\r\n                    id=\"search\"\r\n                />\r\n                <br/>\r\n                <button onClick={handleFormSubmit} className=\"btn btn-primary\">\r\n                  Search\r\n                </button>\r\n              </div>\r\n            </form>\r\n          </div>\r\n        </div>\r\n        <div className={'row'}>\r\n          {books && books.length === 0 ? <div/> :\r\n              <div className={'card col-12 p-3 mt-5 results'}>\r\n                <h1> Results: </h1>\r\n                <div className={'card-body resultsCard'}>\r\n                  {books ? books.map((book, index) => {\r\n                    return (\r\n                        <div key={book.id} className={'row'}>\r\n\r\n                          <div className={'col-3'}>\r\n                            <h3 className={'card-title'}>Title: {book.title}</h3>\r\n                            <p className={'card-title'}>Publisher: {book.publisher ? book.publisher : ''}</p>\r\n                            <p className={'card-title'}>Author: {book.author ? book.author.join(', ') : ''}</p>\r\n                            <img className={'card-image'} src={book?.image} alt={book.title}/>\r\n                          </div>\r\n                          <div className={'col-9'}>\r\n                            <p className={'card-body'}> {book.description} </p>\r\n                          </div>\r\n                          <div className={'col-12'}>\r\n                            <div className={'row'}>\r\n                              <div className={'pt-3 col-10'}>\r\n                                <a href={book.link}> View on Google Books </a>\r\n                              </div>\r\n                              <div className={'col-1'}>\r\n                                <button type=\"button\" className=\"btn btn-primary\" id={book.id}\r\n                                  onClick={handleSaveBtn}> Save </button>\r\n                              </div>\r\n                            </div>\r\n                            <hr/>\r\n                          </div>\r\n                        </div>\r\n                    )\r\n                  }) : <h3> No results found </h3>}\r\n                </div>\r\n              </div>\r\n          }\r\n        </div>\r\n      </div>\r\n  );\r\n}\r\n\r\nexport default SearchForm;\r\n","C:\\Users\\mavki\\Documents\\Bootcamp_Homework\\Week22-Homework\\goolge-books-react\\client\\src\\index.js",[],"C:\\Users\\mavki\\Documents\\Bootcamp_Homework\\Week22-Homework\\goolge-books-react\\client\\src\\containers\\SavedPage\\index.js",[],"C:\\Users\\mavki\\Documents\\Bootcamp_Homework\\Week22-Homework\\goolge-books-react\\client\\src\\components\\Navbar.js",[],"C:\\Users\\mavki\\Documents\\Bootcamp_Homework\\Week22-Homework\\goolge-books-react\\client\\src\\utils\\API.js",["32"],{"ruleId":"33","severity":1,"message":"34","line":1,"column":26,"nodeType":"35","messageId":"36","endLine":1,"endColumn":35},{"ruleId":"37","severity":1,"message":"38","line":8,"column":1,"nodeType":"39","endLine":59,"endColumn":3},"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","import/no-anonymous-d